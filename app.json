{
  "expo": {
    "name": "route_vendor_interface",
    "slug": "route_vendor_interface",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "experimental": {
        "useNewArchitecture": true
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "unstable_reactLegacyComponentNames": [
          "AIRMap",
          "AIRMapCallout",
          "AIRMapCalloutSubview",
          "AIRMapCircle",
          "AIRMapHeatmap",
          "AIRMapLocalTile",
          "AIRMapMarker",
          "AIRMapOverlay",
          "AIRMapPolygon",
          "AIRMapPolyline",
          "AIRMapUrlTile",
          "AIRMapWMSTile"
        ]
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.renet_perez.route_vendor_interface",
      "runtimeVersion": "1.0.0",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCkM6bIRELzLGyawIg7GTuD9C3c8Mgsm3s"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isAndroidBackgroundLocationEnabled": "Allow $(PRODUCT_NAME) to use your location.",
          "isAndroidForegroundServiceEnabled": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "03a7605b-325c-4f2c-9133-42e0f0fcc4e4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/03a7605b-325c-4f2c-9133-42e0f0fcc4e4"
    }
  }
}
